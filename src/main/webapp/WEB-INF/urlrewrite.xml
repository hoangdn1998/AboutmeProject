<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
	<rule>
	<from>/(.+)/(.+)-([0-9]+)$</from> 
	<to>/news/detail?news_id=$3</to>
	</rule>
	<rule>
	<from>/(.+)-([0-9]+)$</from>
	<to>/cats?cat_id=$2</to>
	</rule>
</urlrewrite>

